import "std/io.cerm";

pub fn main() {
    var i u32 = 1;

    while i < 16 {
        if i % 3 == 0 {
            if i % 5 == 0 {
                std::io::print("FizzBuzz\n");
            } else {
                std::io::print("Fizz\n");
            }
        } else if i % 5 == 0 {
            std::io::print("Buzz\n");
        } else {
            std::io::numprint(i);
            std::io::print("\n");
        }

        i += 1;
    }
}
