const SYS_write = 1;

const STDOUT = 1;

fn write(fd u32, buf [*]const u8, count u64) u64 {
    return asm {
        "syscall"
        : "={rax}" (u64)
        : "{rax}" (SYS_write),
          "{rdi}" (fd),
          "{rsi}" (buf),
          "{rdx}" (count),
    };
}

fn main() {
    write(STDOUT, "Hello, World!\n", 14);
}
