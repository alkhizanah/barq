const SYS_exit_group = 231;

fn syscall1(code usize, arg1 usize) usize {
    return asm {
        "syscall"
        : "={rax}" (usize)
        : "{rax}" (code),
          "{rdi}" (arg1),
        : "rcx", "r11", "memory"
    };
}

extern fn main();

fn _start() {
    main();

    syscall1(SYS_exit_group, 0);
}
